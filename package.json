{
  "name": "fluxojs",
  "version": "0.0.0",
  "description": "Fluxo de caixa",
  "main": "app.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha",
    "eslint": "eslint --ext '.js,.vue' .",
